{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dev-gadgets",
  "title": "Dev Gadgets",
  "description": "A set of gadgets for developers",
  "icon": "command-icon.png",
  "author": "okuramasafumi",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Dev Gadgets",
      "description": "Show dev gadgets available",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8"
  },
  "devDependencies": {
    "@types/node": "20.4.2",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.3",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
